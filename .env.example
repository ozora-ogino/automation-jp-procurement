# PostgreSQL設定
POSTGRES_HOST=postgres
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow
POSTGRES_DB=automation-jp-procurement
POSTGRES_PORT=5432

# Airflow設定
AIRFLOW_WEBSERVER_PORT=3031
AIRFLOW_ADMIN_USERNAME=admin
AIRFLOW_ADMIN_PASSWORD=admin
AIRFLOW_ADMIN_FIRSTNAME=ozora
AIRFLOW_ADMIN_LASTNAME=ogino
AIRFLOW_ADMIN_EMAIL=ozora@example.com

# Airflow設定値
AIRFLOW_EXECUTOR=LocalExecutor
AIRFLOW_FERNET_KEY=""
AIRFLOW_DAGS_PAUSED_AT_CREATION=true
AIRFLOW_LOAD_EXAMPLES=false

# OpenAI API設定
OPENAI_API_KEY=
EMBEDDING_MODEL=text-embedding-3-large
LLM_MODEL=gpt-4o-2024-11-20

BACKEND_URL=http://api:8002

# NJSS Crawler Credentials
NJSS_USERNAME=your_username_here
NJSS_PASSWORD=your_password_here

# NJSS Crawler Settings
NJSS_DOWNLOAD_DIR=/opt/airflow/downloads
NJSS_DEBUG_MODE=false

# Browser settings
CRAWLER_HEADLESS=true
CRAWLER_TIMEOUT=30000
BROWSER_WIDTH=1920
BROWSER_HEIGHT=1080

# Crawling settings
MAX_PAGES=100
PAGE_DELAY=1.0
RETRY_ATTEMPTS=3
RETRY_DELAY=5.0

# Output settings
OUTPUT_DIR=/opt/airflow/output
LOG_LEVEL=INFO
